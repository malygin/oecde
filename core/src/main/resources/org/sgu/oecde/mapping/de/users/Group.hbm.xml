<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="oecdo_test">
  <joined-subclass dynamic-update="true" extends="org.sgu.oecde.core.users.StudentGroup" name="org.sgu.oecde.de.users.Group" table="de_group">
    <key column="id" not-null="true" on-delete="cascade"/>
    <many-to-one class="org.sgu.oecde.de.education.City" fetch="join" insert="false" name="city" not-null="true"/>
    <many-to-one class="org.sgu.oecde.core.education.Speciality" fetch="join" lazy="false" name="speciality" not-null="true"/>
  </joined-subclass>
</hibernate-mapping>
