    <many-to-one class="org.sgu.oecde.core.education.Discipline" column="id_discipline" fetch="join" insert="false" name="discipline" not-null="true" update="false"/>
    <many-to-one class="org.sgu.oecde.core.education.Speciality" column="id_speciality" fetch="join" insert="false" name="speciality" not-null="true" update="false"/>
  </subclass>
</hibernate-mapping>
