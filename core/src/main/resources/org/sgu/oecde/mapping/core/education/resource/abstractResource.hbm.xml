<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="oecdo_test">
  <class dynamic-update="true" name="org.sgu.oecde.core.education.resource.AbstractResource" select-before-update="false" table="abstract_resource">
    <id name="id" type="long"/>
    <property insert="false" name="title" type="string"/>
    <property insert="false" name="description" type="string"/>
    <property insert="false" name="version" scale="3" type="integer"/>
    <property insert="false" name="visible" scale="1" type="boolean"/>
    <property column="date_last_change" insert="false" length="19" name="dateLastchange" type="string"/>
    <property column="access_resource" insert="false" name="accessResource">
      <type name="org.sgu.oecde.core.GenericEnumUserType">
        <param name="enumClass">org.sgu.oecde.core.education.resource.AccessResource</param>
        <param name="identifierMethod">toName</param>
        <param name="valueOfMethod">parse</param>
      </type>
    </property>
  </class>
</hibernate-mapping>
