<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="oecdo_test">
  <class mutable="false" name="org.sgu.oecde.core.education.Module" table="module">
    <cache usage="read-only"/>
    <id name="id" type="int"/>
    <property insert="false" name="name" type="string"/>
    <property insert="false" name="description" type="string"/>
    <many-to-one fetch="select" foreign-key="fk_mod_umk" insert="false" lazy="no-proxy" name="umk" not-null="true"/>
    <set batch-size="5" cascade="none" fetch="select" mutable="false" name="resources" order-by="resource_id asc" table="module_resources">
      <cache usage="read-only"/>
      <key column="module" foreign-key="fk_module_res_module" not-null="true"/>
      <many-to-many class="org.sgu.oecde.core.education.resource.AbstractResource" column="resource_id" foreign-key="fk_module_res"/>
    </set>
  </class>
</hibernate-mapping>