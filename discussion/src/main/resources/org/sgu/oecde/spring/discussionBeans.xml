<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       ">
<bean id="nodeDao" p:sessionFactory-ref="sessionFactory" class="org.sgu.oecde.discussion.dao.NodeDao" />
<bean id="rootDao" p:sessionFactory-ref="sessionFactory" class="org.sgu.oecde.discussion.dao.RootDao" />
<bean scope="prototype" id="discussionDaoProxy" p:rootDao-ref="rootDao" class="org.sgu.oecde.discussion.DiscussionDaoProxy" />

</beans>