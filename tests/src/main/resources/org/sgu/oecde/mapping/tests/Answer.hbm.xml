<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="oecdo_test">
  <class dynamic-update="true" name="org.sgu.oecde.tests.Answer" table="answers">
    <id name="id" type="long"/>
    <property insert="false" name="title" type="string"/>
    <property column="right_answer" insert="false" name="rightAnswer" type="string"/>
  </class>
</hibernate-mapping>
