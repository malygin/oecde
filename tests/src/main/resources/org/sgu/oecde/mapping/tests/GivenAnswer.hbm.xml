<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="oecdo_test">
  <class mutable="false" name="org.sgu.oecde.tests.GivenAnswer" table="given_answer">
    <id name="id" type="long" unsaved-value="0">
      <generator class="native">
        <param name="sequence">gvn_answer_seq</param>
      </generator>
    </id>
    <property column="given_answer" name="givenAnswer" type="string"/>
    <many-to-one class="org.sgu.oecde.tests.Answer" column="right_answer" fetch="join" foreign-key="fk_gvanswr_answr" name="rightAnswer" not-null="true"/>
    <many-to-one class="org.sgu.oecde.tests.AnsweredQuestion" column="question_result_id" fetch="select" foreign-key="fk_gvanswr_quest" lazy="proxy" name="answeredQuestion" not-null="true"/>
  </class>
</hibernate-mapping>
