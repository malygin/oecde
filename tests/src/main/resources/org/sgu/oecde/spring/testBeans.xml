<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
       ">
<bean id="testDao" p:type="org.sgu.oecde.tests.TestEntity"  parent="updateDao" class="org.sgu.oecde.tests.dao.TestDao" />
<bean id="testAttemptDao" p:type="org.sgu.oecde.tests.TestAttempt"  parent="basicDao" class="org.sgu.oecde.tests.dao.TestAttemptDao" />
<bean scope="prototype" id="testFilter" class="org.sgu.oecde.tests.filters.Filter" />
<bean id="pointsFactory" class="org.sgu.oecde.tests.TestsPointsFactory" />
<bean id="testAttemptService" p:attemptsDao-ref="testAttemptDao" p:testDao-ref="testDao" class="org.sgu.oecde.tests.TestAttemptService" />
</beans>