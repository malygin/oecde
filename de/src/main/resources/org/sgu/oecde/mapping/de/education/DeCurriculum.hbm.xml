<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping schema="oecdo_test">
  <subclass discriminator-value="de" extends="org.sgu.oecde.core.education.AdvancedCurriculum" name="org.sgu.oecde.de.education.DeCurriculum">
    <join table="de_curriculum">
      <key column="id" foreign-key="fk_curriculum_curriculum" not-null="true" on-delete="cascade"/>
      <property column="control_works_paper_only" insert="false" name="controlWorksPaperOnly" scale="1" type="boolean"/>
      <property column="lab_works_number" insert="false" name="labWorksNumber" scale="2" type="integer"/>
      <property column="term_papers_number" insert="false" name="termPapersNumber" scale="1" type="integer"/>
      <property insert="false" name="selected" scale="1" type="boolean"/>
      <property insert="false" length="20" name="cycle" type="string"/>
    </join>
  </subclass>
</hibernate-mapping>
